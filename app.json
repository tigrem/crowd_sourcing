{
  "expo": {
    "name": "Call Quality Monitor",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "callqualitymonitor",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.callqualitymonitor"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.callqualitymonitor",
      "permissions": [
        "READ_PHONE_STATE",
        "READ_CALL_LOG",
        "FOREGROUND_SERVICE",
        "READ_CONTACTS"
      ]
    },
    "web": {
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-video",
      "expo-build-properties"
    ]
  }
}
